{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Top 10 Countries with Highest Air Pollution Death Rates in 2016",
  "width": "300",
  "height": "300",
  "data": {
    "url": "https://raw.githubusercontent.com/MahirAbrar/assn2/main/data/top_10_2016.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "Entity",
      "type": "nominal",
      "title": "Country",
      "sort": "-y"
    },
    "y": {
      "field": "Air pollution (total) (IHME, 2019)",
      "type": "quantitative",
      "title": "Air Pollution Death Rate"
    },
    "color": {
      "field": "Entity",
      "type": "nominal"
    }
  }
}
